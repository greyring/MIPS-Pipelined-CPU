INTHANDLE:
lui $k0, 0xb000;
lw  $k1, 0x14($k0);#read keyboard
lhu $k1, 4($k0);#get cursor position
sll $k1, 2;
li  $k0, 0xb0002000;
add $k0, $k0, $k1;#address in text space
addi $k1, $zero, 0x3f;
sb  $k1, 3($k0);#set text front color
addi $k1, $zero, 3;
sh  $k1, 0($k0);#set text code
lui $k0, 0xb000;
lhu $k1, 4($k0);
addi $k1, $k1, 1;
sh  $k1, 4($k0);#move cursor to the next position
eret;
